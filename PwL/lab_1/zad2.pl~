counter(X, [X|T], A, Acc) :- Anew is A+1, counter(X, T, Anew, Acc).
counter(X, [H|T], A, Acc) :- counter(X, T, A, Acc), X \= H.
counter(_, [], Acc, Acc).

jednokrotnie(X, L) :- counter(X, L, 0, Acc), Acc is 1.
dwukrotnie(X, L) :- counter(X, L, 0, Acc), Acc is 2.




























































































































































